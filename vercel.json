{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {"handle": "filesystem"},
      { "src": "/auth/login", "dest": "/server.js" },
      { "src": "/auth/(.*)", "dest": "/server.js" },
      { "src": "/video/dashboard", "dest": "/server.js" },
      { "src": "/video/new_video", "dest": "/server.js" },
      { "src": "/video/database", "dest": "/server.js" },
      { "src": "/resource/(.*)", "dest": "/resource/$1" },
      { "src": "/views/(.*)", "dest": "/views/$1" },
      { "src": "/(.*)", "dest": "/server.js" }
    ]
  }
  